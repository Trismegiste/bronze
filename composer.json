{
    "name": "trismegiste/bronze",
    "description": "Code your Proof Of Concept with swag",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/http-foundation": "^6.2",
        "symfony/routing": "^6.2",
        "twig/twig": "^3.0",
        "symfony/http-kernel": "^6.2",
        "symfony/form": "^6.2",
        "symfony/twig-bridge": "^6.2",
        "symfony/translation": "^6.2",
        "trismegiste/strangelove-bundle": "^2.0",
        "symfony/security-csrf": "^6.2",
        "symfony/validator": "^6.2",
        "symfony/console": "^6.2",
        "wikimedia/parsoid": "^0.16.1",
        "monolog/monolog": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Trismegiste\\Bronze\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Trismegiste\\Bronze\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symfony/browser-kit": "^6.2"
    }
